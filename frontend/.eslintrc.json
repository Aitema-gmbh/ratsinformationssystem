{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "no-console": [
      "warn",
      {
        "allow": [
          "warn",
          "error"
        ]
      }
    ],
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_"
      }
    ],
    "prefer-const": "error",
    "eqeqeq": [
      "error",
      "always"
    ],
    "react/no-unescaped-entities": "warn"
  }
}
